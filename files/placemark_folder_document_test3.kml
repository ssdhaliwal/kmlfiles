<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" 
	xmlns:gx="http://www.google.com/kml/ext/2.2" 
	xmlns:kml="http://www.opengis.net/kml/2.2" 
	xmlns:atom="http://www.w3.org/2005/Atom">
	<Document>
		<name>Folders</name>
		<Placemark>
			<name>Polygon in root document 1</name>
			<styleUrl>#orange-5px</styleUrl>
			<LineString>
				<tessellate>1</tessellate>
				<coordinates>-135,30,500000 -80,30,500000</coordinates>
			</LineString>
		</Placemark>
		<Folder>
			<name>Folder one</name>
			<open>1</open>
			<Document>
				<Folder>
					<name>sub folder 1</name>
					<open>1</open>
					<Document>
						<Placemark>
							<name>Path in subfolder</name>
							<styleUrl>#orange-5px</styleUrl>
							<LineString>
								<tessellate>1</tessellate>
								<coordinates>-122.378927,37.826793,0 1,1,0 1,2,0 -122.000,37.002,0</coordinates>
							</LineString>
						</Placemark>
						<Placemark>
							<name>Path in subfolder 1</name>
							<styleUrl>#exampleBalloonStyle</styleUrl>
							<Point>
								<tessellate>1</tessellate>
								<coordinates>1,1,0</coordinates>
							</Point>
						</Placemark>
						<Placemark>
							<name>Path in subfolder 2</name>
							<styleUrl>#exampleStyleDocument</styleUrl>
							<Point>
								<tessellate>1</tessellate>
								<coordinates>1,2,0</coordinates>
							</Point>
						</Placemark>
						<Placemark>
							<name>Path in subfolder 3</name>
							<styleUrl>#randomColorIcon</styleUrl>
							<Point>
								<tessellate>1</tessellate>
								<coordinates>-122.000,37.002,0</coordinates>
							</Point>
						</Placemark>
						<Placemark>
							<name>Path in folder one</name>
							<styleUrl>#randomLabelColor</styleUrl>
							<Point>
								<tessellate>1</tessellate>
								<coordinates>-121.998,37.0078,0</coordinates>
							</Point>
						</Placemark>
						<Placemark>
							<name>Polygon in 2nd folder</name>
							<styleUrl>#orange-5px</styleUrl>
							<Point>
								<tessellate>1</tessellate>
								<coordinates>-122.377588,37.830266,0</coordinates>
							</Point>
						</Placemark>
					</Document>
				</Folder>
				<Folder>
					<name>sub folder 2</name>
					<open>1</open>
					<Placemark>
						<name>Path in folder one</name>
						<styleUrl>#exampleBalloonStyle</styleUrl>
						<Point>
							<tessellate>1</tessellate>
							<coordinates>-122.377830,37.830445,0</coordinates>
						</Point>
					</Placemark>
					<Placemark>
						<name>Path in subfolder 3</name>
						<styleUrl>#orange-5px</styleUrl>
						<Point>
							<tessellate>1</tessellate>
							<coordinates>-122.377576,37.830631,0</coordinates>
						</Point>
					</Placemark>
					<Placemark>
						<name>Polygon in 2nd folder</name>
						<styleUrl>#linestyleExample</styleUrl>
						<Point>
							<tessellate>1</tessellate>
							<coordinates>-122.377840,37.830642,0</coordinates>
						</Point>
					</Placemark>
				</Folder>
				<Placemark>
					<name>Polygon in main folder</name>
					<styleUrl>#randomColorIcon</styleUrl>
					<LineString>
						<tessellate>1</tessellate>
						<coordinates>-122.377830,37.830445,0</coordinates>
					</LineString>
				</Placemark>
			</Document>
		</Folder>
		<Placemark>
			<name>Polygon in root document 2</name>
			<styleUrl>#myDefaultStyles</styleUrl>
			<LineString>
				<tessellate>1</tessellate>
				<coordinates>-90.86948943473118,48.25450093195546,0 -122.377885,37.830379,0</coordinates>
			</LineString>
		</Placemark>
		<Folder>
			<name>Folder two</name>
			<open>1</open>
			<Placemark>
				<name>Polygon in 2nd folder</name>
				<styleUrl>#exampleStyleDocument</styleUrl>
				<LineString>
					<tessellate>1</tessellate>
					<coordinates>-122.378009,37.830128,0 -122.377885,37.830379,0</coordinates>
				</LineString>
			</Placemark>
			<Placemark>
				<name>Path in subfolder 3</name>
				<styleUrl>#linestyleExample</styleUrl>
				<LineString>
					<tessellate>1</tessellate>
					<coordinates>-122.4425587930444,37.80666418607323,0 -122.4428379594768,37.80663578323093,0</coordinates>
				</LineString>
			</Placemark>
		</Folder>
		<Placemark>
			<name>Polygon in root multi document 3</name>
			<styleUrl>#myDefaultStyles</styleUrl>
			<MultiGeometry>
				<Point>
					<tessellate>1</tessellate>
					<coordinates>-135,30,500000 -80,30,500000</coordinates>
				</Point>
				<LineString>
					<tessellate>1</tessellate>
					<coordinates>-122.378009,37.830128,0 -122.377885,37.830379,0</coordinates>
				</LineString>
				<Point>
					<tessellate>1</tessellate>
					<coordinates>1,1,0</coordinates>
				</Point>
			</MultiGeometry>
		</Placemark>
		<Placemark>
			<name>Polygon in root document 3</name>
			<styleUrl>#myDefaultStyles</styleUrl>
			<Point>
				<tessellate>1</tessellate>
				<coordinates>-122.4425509770566,37.80662588061205,0 -122.4428340530617,37.8065999493009,0</coordinates>
			</Point>
		</Placemark>
		<Style id="orange-5px">
			<LineStyle>
				<color>ff00aaff</color>
				<width>5</width>
			</LineStyle>
		</Style>
		<Style id="exampleStyleDocument">
			<LabelStyle>
				<color>ff0000cc</color>
			</LabelStyle>
		</Style>
		<Style id="trailhead-balloon-template">
			<BalloonStyle>
				<text><![CDATA[             <h2>My favorite trails!</h2><br/><br/>             The $[TrailHeadType/TrailHeadName/displayName] is <i>$[TrailHeadType/TrailHeadName]</i>.             The trail is $[TrailHeadType/TrailLength] miles.<br/>             The climb is $[TrailHeadType/ElevationGain] meters.<br/><br/>           ]]></text>
			</BalloonStyle>
		</Style>
		<Style id="myDefaultStyles">
			<IconStyle>
				<color>a1ff00ff</color>
				<scale>1.399999976158142</scale>
				<Icon>
					<href>http://myserver.com/icon.jpg</href>
				</Icon>
			</IconStyle>
			<LabelStyle>
				<color>7fffaaff</color>
				<scale>1.5</scale>
			</LabelStyle>
			<LineStyle>
				<color>ff0000ff</color>
				<width>15</width>
			</LineStyle>
			<PolyStyle>
				<color>7f7faaaa</color>
				<colorMode>random</colorMode>
			</PolyStyle>
		</Style>
		<Style id="normalState">
			<IconStyle>
				<scale>1.0</scale>
				<Icon>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon55.png</href>
				</Icon>
			</IconStyle>
			<LabelStyle>
				<scale>1.0</scale>
			</LabelStyle>
		</Style>
		<Style id="highlightState">
			<IconStyle>
				<Icon>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon60.png</href>
				</Icon>
				<scale>1.1</scale>
			</IconStyle>
			<LabelStyle>
				<scale>1.1</scale>
				<color>ff0000c0</color>
			</LabelStyle>
		</Style>
		<StyleMap id="styleMapExample">
			<Pair>
				<key>normal</key>
				<styleUrl>#normalState</styleUrl>
			</Pair>
			<Pair>
				<key>highlighted</key>
				<styleUrl>#highlightState</styleUrl>
			</Pair>
		</StyleMap>
		<Style id="exampleBalloonStyle">
			<BalloonStyle>				<!-- a background color for the balloon -->
				<bgColor>ffffffbb</bgColor>				<!-- styling of the balloon text -->
				<text><![CDATA[       <b><font color="#CC0000" size="+3">$[name]</font></b><br/><br/><font face="Courier">$[description]</font><br/><br/>       Extra text that will appear in the description balloon       <br/><br/><!-- insert the to/from hyperlinks -->       $[geDirections]       ]]></text>
			</BalloonStyle>
		</Style>
		<Style id="randomColorIcon">
			<IconStyle>
				<color>ff00ff00</color>
				<colorMode>random</colorMode>
				<scale>1.1</scale>
				<Icon>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon21.png</href>
				</Icon>
			</IconStyle>
		</Style>
		<Style id="randomLabelColor">
			<LabelStyle>
				<color>ff0000cc</color>
				<colorMode>random</colorMode>
				<scale>1.5</scale>
			</LabelStyle>
		</Style>
		<Style id="linestyleExample">
			<LineStyle>
				<color>7f0000ff</color>
				<width>4</width>
			</LineStyle>
		</Style>
		<Style id="examplePolyStyle">
			<PolyStyle>
				<color>ff0000cc</color>
				<colorMode>random</colorMode>
			</PolyStyle>
		</Style>
		<Style id="bgColorExample">
			<ListStyle>
				<bgColor>ff336699</bgColor>
			</ListStyle>
		</Style>
		<Style id="checkHideChildrenExample">
			<ListStyle>
				<listItemType>checkHideChildren</listItemType>
				<ItemIcon id="1">
					<state>open</state>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon21.png</href>
				</ItemIcon>
				<ItemIcon id="2">
					<state>partial</state>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon22.png</href>
				</ItemIcon>
				<ItemIcon id="3">
					<state>closed</state>
					<href>http://maps.google.com/mapfiles/kml/pal3/icon23.png</href>
				</ItemIcon>
			</ListStyle>
		</Style>
		<Style id="radioFolderExample">
			<ListStyle>
				<listItemType>radioFolder</listItemType>
			</ListStyle>
		</Style>
	</Document>
</kml>