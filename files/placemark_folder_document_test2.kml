	<Document>
		<name>Folders</name>
		<Style id="transRedPoly">
			<LineStyle>
				<width>4</width>
			</LineStyle>
			<PolyStyle>
				<color>#7d0000ff</color>
			</PolyStyle>
		</Style>
		<Style id="transBluePoly">
			<LineStyle>
				<width>4</width>
			</LineStyle>
			<PolyStyle>
				<color>#7d0000ff</color>
				<fill>1</fill>
				<outline>1</outline>
			</PolyStyle>
		</Style>
		<Placemark>
			<name>Point in root document 1</name>
			<styleUrl>#myDefaultStyles</styleUrl>
			<Point>
				<tessellate>1</tessellate>
				<coordinates>-135,30,500000 -80,30,500000</coordinates>
			</Point>
		</Placemark>
